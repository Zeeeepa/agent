# OpenAI API key
OPENAI_API_KEY=

# Initial pulse value: the higher it is, the more errors the agent can tolerate before stopping
PULSE=120

# Timeout (in seconds) to wait for user input before the agent starts thinking by itself
TIMEOUT=300

# Optional SOCKS5/HTTP proxy for OpenAI requests.
# PROXY=socks5://127.0.0.1:12334

# Model override (optional). If not set, SDK default is used.
# Example: gpt-5
OPENAI_MODEL=gpt-4.1

# Optional: Pre-existing OpenAI Vector Store ID for RAG queries
# Used by jinx/rag_service.py when attaching your knowledge base to responses.
# Obtain from https://platform.openai.com/storage/vector-stores
OPENAI_VECTOR_STORE_ID=