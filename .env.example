# OpenAI API key
OPENAI_API_KEY=

# Initial pulse value: the higher it is, the more errors the agent can tolerate before stopping
PULSE=120

# Timeout (in seconds) to wait for user input before the agent starts thinking by itself
TIMEOUT=300

